---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>wwwlife</title>
 <link href="http://wwwlife.info/atom.xml" rel="self"/>
 <link href="http://wwwlife.info/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://wwwlife.info/</id>
 <author>
   <name>wwwlife</name>
   <email>1989wg@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://wwwlife.info{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://wwwlife.info{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
